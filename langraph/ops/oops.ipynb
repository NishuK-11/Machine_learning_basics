   "source": [
    "class Car:\n",
    "    def __init__(self, brand, color):\n",
    "        self.brand = brand\n",
    "        self.color = color\n",
    "\n",
    "    def show(self):\n",
    "        print(f\"Brand: {self.brand}, Color: {self.color}\")\n",
    "\n",
    "# create objects\n",
    "car1 = Car(\"Tata\", \"Red\")\n",
    "car2 = Car(\"BMW\", \"Black\")\n",
    "\n",
    "car1.show()\n",
    "car2.show()\n"
   ]
  }
